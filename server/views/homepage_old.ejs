<% if(!locals.trainPlans || !locals.trainPlans.length) {%>
    <h3 style="color:red;">Błąd ładowania planów treningowych.</h3>
<% } else {%>
    <section class="bg-faded pb50">
        <div class="container">
            <% let cardClassWidth = (12 / locals.trainPlans.length); %>
            <% if(cardClassWidth > 6){cardClassWidth = 6;} %>
            <% if(cardClassWidth < 3){cardClassWidth = 3;} %>
            <% const cardClass = "col-md-" + cardClassWidth; %>
            <div class="row">
            <% 
                let maxPointsCount = 0;
                for(let i = 0; i < locals.trainPlans.length; i++) {
                    const pointsCount = (locals.trainPlans[i].description || '').split(/\n/).length;
                    if(maxPointsCount < pointsCount){
                        maxPointsCount = pointsCount;
                    }
                }
            %>
            <% for(let i = 0; i < locals.trainPlans.length; i++) {%>
                <% let plan = locals.trainPlans[i]; %>
                <div class="<%- cardClass %> mb40 wow fadeInUp" data-wow-delay=".<%- i+1 %>s">
                    <form action="/addItemToCart" method="POST">
                        <input type="hidden" name="trainingPlan" value="<%- plan.id %>">
                        <div class="pricing-card <%- plan.isRecomended ? 'active':'' %>">
                            <header class="pricing-header clearfix">
                                <div class="price-title"><%- plan.name %></div>
                                <div class="float-right price">
                                    <% if(plan.priceOld) {%>
                                    <span class='old-price'><strike><%- plan.priceOld %></strike> <sup>zł</sup> </span>
                                    <% } %>
                                    <%- plan.price %> <sup>zł</sup>
                                </div>                                
                            </header>
                            <div class="pricing-features">
                                <ul class="list-unstyled">
                                    <li>
                                       Treningów w cenie: <b><%- plan.trainsCount %></b>
                                    </li>
                                    <% const features = (plan.description || '').split(/\n/); %>
                                    <% for(let k = 0; k < features.length; k++) {%>
                                    <li>
                                        <%- features[k] %>   
                                    </li>
                                    <% } %>
                                    <% for(let k = 0; k < maxPointsCount - features.length; k++) {%>
                                    <li class='empty-point'>
                                        &nbsp;
                                    </li>
                                    <% } %>
                                </ul>                              
                            </div><!--pricing features-->
                            <footer class="pricing-footer text-center">                                    
                                <button type="submit" class="btn btn-outline-info">Dodaj do koszyka</button>
                            </footer>
                        </div><!--/pricing card-->
                    </form>
                </div>
            <% } %>
            </div>      
            <div class="row">
                <div class="col-lg-12 text-right">
                    <span style="margin-right: 20px;" >Gdzie i w jakich godzinach mozemy trenować?</span>
                    <a style="margin-top: -9px;" href="/trainings" class="btn btn-primary pull-right">Sprawdź</a>
                </div>
            </div>      
        </div>        
    </section>

<% } %>

