    $('body').on('change','#type',function(){
    	if(this.value == 'consultant'){
    		$('.place-row').addClass('hidden');
            $('.place-row select').hide();
    	}else{    		
            $('.place-row select').show();
    		$('.place-row').removeClass('hidden');
    	}
    });
</script>