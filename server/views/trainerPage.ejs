            lightboxTitleSrc: 'data-title',
            lightboxCounter: '<div class="cbp-popup-lightbox-counter">{{current}} z {{total}}</div>'
        });

        $('body').on('click','.more-items',function(){
            var me = $(this);
            me.siblings().show();
            var lastItem = me.siblings().last();
